package exam_trials;

public class CountVolwels {
	public static void main(String[] args) {
		String a="lets have a break";
		char ch[]=a.toCharArray();
		int count=0;
		for(char c:ch) {
			if(c=='a'||c=='i'||c=='e'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U') {
				count++;
				
			}
			
		}
		System.out.println("the count of vowels are " +count);
		
	}

}
